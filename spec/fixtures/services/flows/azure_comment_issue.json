      "Microsoft.VSTS.Scheduling.StoryPoints": 3.0,
      "Microsoft.VSTS.Common.StateChangeDate": "2021-08-18T14:36:46.16Z",
      "Microsoft.VSTS.Common.ActivatedDate": "2021-08-18T14:36:46.16Z",
      "Microsoft.VSTS.Common.ActivatedBy": "René Monroy \u003crene.monroy@codelitt.com\u003e",
      "Microsoft.VSTS.Common.Priority": 2,
      "Microsoft.VSTS.Common.StackRank": 999687722.0,
